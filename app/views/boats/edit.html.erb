<%= form_with url: "/boats/#{@boat.id}", method: :patch, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :motor_powered %>
<%= form.text_field :motor_powered %>
<%= form.label :crew_size %>
<%= form.text_field :crew_size %>
<%= form.submit 'Update Boat' %>
<% end %>
