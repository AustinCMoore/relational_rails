<h1>Aquariums!</h1>
<%= link_to "New Aquarium", "/aquariums/new" %>
<% @aquariums.created_first.each do |aquarium| %>
  <h4><%= aquarium.name %> </h4>
  <h4> Record Created At: <%= aquarium.created_at %></h4>
  <%= link_to "Delete #{aquarium.name}", "/aquariums/#{aquarium.id}", method: :delete %>
  <%= link_to "Edit #{aquarium.name}", "aquariums/#{aquarium.id}/edit" %>
<% end %>
