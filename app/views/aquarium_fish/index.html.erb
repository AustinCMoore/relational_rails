<h1> <%= "#{@aquarium.name}" %> Fish </h1>
  <% @fishies.each do |fish| %>
<ul>
    <li><h3><%= fish.name  %></h3></li>
    <li><p> Bioluminescent?: <%= fish.bioluminescent  %></p></li>
    <li><p> Quantity: <%= fish.quantity  %></p></li>
</ul>
  <% end %>
<%= link_to "Create Fish", "/aquariums/#{@aquarium.id}/fish/new" %>
<%= link_to "Alphabetize Fish", "/aquariums/#{@aquarium.id}/fish?sort=asc" %>