<%= form_with url: "/aquariums/#{@aquarium.id}/fish", method: :post, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :quantity %>
<%= form.text_field :quantity %>
<%= form.label :bioluminescent %>
<%= form.text_field :bioluminescent %>
<%= form.submit 'Create Fish' %>
<% end %>